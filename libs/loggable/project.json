{
  "root": "libs/loggable",
  "sourceRoot": "libs/loggable/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/loggable",
        "tsConfig": "libs/loggable/tsconfig.lib.json",
        "packageJson": "libs/loggable/package.json",
        "main": "libs/loggable/src/index.ts",
        "assets": ["libs/loggable/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/loggable/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/loggable"],
      "options": {
        "jestConfig": "libs/loggable/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
