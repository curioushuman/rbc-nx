{
  "root": "libs/common-config",
  "sourceRoot": "libs/common-config/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/common-config",
        "tsConfig": "libs/common-config/tsconfig.lib.json",
        "packageJson": "libs/common-config/package.json",
        "main": "libs/common-config/src/index.ts",
        "assets": ["libs/common-config/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/common-config/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/common-config"],
      "options": {
        "jestConfig": "libs/common-config/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
